{
  /* Titanic's End Fixture File */
  label: "Panel PEA",
  tags: [ "PEA", "panel", "Panel", "m19" ],

  parameters: {
    "xOffset": { default: 9.530516779280937, type: float, description: "Adjust X position within the plane of the panel. Use to fine-tune position after installation" },
    "yOffset": { default: 6.0, type: float, description: "Adjust Y position within the plane of the panel. Use to fine-tune position after installation" },
  
    /* Modify HOST and CONTROLLER OUTPUT default values HERE to assign one of this panel's strands to a
       new controller output. Then click [Reload Fixture File] at the bottom of the Inspector section in Chromatik.
       CAUTION: LXM model files containing this fixture might not update to the new defaults. */

    "strand1host": { default: "10.7.19.112", type: "string", label: "S1 Host", description: "Strand 1 Controller IP address or hostname" },
    "strand1output": { default: 1, type: "int", min: 1, max: 4, label: "S1 Output", description: "Strand 1 Controller Output Number 1-4" },
    "strand2host": { default: "10.7.19.112", type: "string", label: "S2 Host", description: "Strand 2 Controller IP address or hostname" },
    "strand2output": { default: 2, type: "int", min: 1, max: 4, label: "S2 Output", description: "Strand 2 Controller Output Number 1-4" },
    "strand3host": { default: "10.7.19.112", type: "string", label: "S3 Host", description: "Strand 3 Controller IP address or hostname" },
    "strand3output": { default: 3, type: "int", min: 1, max: 4, label: "S3 Output", description: "Strand 3 Controller Output Number 1-4" },

    "artnetSequence": { default: false, type: "boolean", label: "ArtNet Sequence", description: "Enable ArtNet sequence packets" },
    
    /* Debug tools */
    "onCar": { default: true, type: "boolean", label: "On Car", description: "True = Locate panel to its position on the car, False = Locate to origin" }
  },
  
  transforms: [
    /* Transpose starting corner to Vertex0 */
    { x: "-288.0000", enabled: "$onCar" },
    { y: "12.0000", enabled: "$onCar" },
    { z: "66.0000", enabled: "$onCar" },

    /* Rotate to plane of panel */
    { yaw: "-12.2022", enabled: "$onCar" },
    { roll: "0.0000", enabled: "$onCar" },
    { pitch: "8.1670", enabled: "$onCar" },

    /* X-Y adjustments within plane of panel */
    { x: "$xOffset" },
    { y: "$yOffset" }    
  ],

  meta: {
    "panelId": "PEA",
    "v0": "116",
    "v1": "9",
    "v2": "114",
    "edge1": "114-116",
    "edge2": "9-114",
    "edge3": "9-116",
    "leadingEdge": "116->9",
    "module": "19"
  },

  components: [
    /* Looking at the back of panel, starts from LEFT
       Looking at the front of panel, starts from RIGHT */

    { type: "panelRow", row: "0", offset: "0", numPoints: "49" },
    { type: "panelRow", row: "1", offset: "0", numPoints: "47" },
    { type: "panelRow", row: "2", offset: "0", numPoints: "46" },
    { type: "panelRow", row: "3", offset: "0", numPoints: "44" },
    { type: "panelRow", row: "4", offset: "0", numPoints: "42" },
    { type: "panelRow", row: "5", offset: "0", numPoints: "41" },
