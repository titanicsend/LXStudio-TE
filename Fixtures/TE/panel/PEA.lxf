    { type: "panelRow", row: "20", offset: "2", numPoints: "14" },
    { type: "panelRow", row: "21", offset: "2", numPoints: "12" },
    { type: "panelRow", row: "22", offset: "3", numPoints: "10" },
    { type: "panelRow", row: "23", offset: "3", numPoints: "8" },
    { type: "panelRow", row: "24", offset: "3", numPoints: "6" },
    { type: "panelRow", row: "25", offset: "3", numPoints: "5" },
    { type: "panelRow", row: "26", offset: "3", numPoints: "3" },
    { type: "panelRow", row: "27", offset: "3", numPoints: "1" }
