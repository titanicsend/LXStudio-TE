{
  /* HiRes grid for capturing TE Output */
  label: "HRGrid",
  tags: [
    "grid"
  ],
  parameters: {
    width: {
      type: "int",
      default: 640,
      min: 10,
      max: 10000,
      label: "Width",
      description: "Width (in Pixels)"
    },
    height: {
      type: "int",
      default: 480,
      min: 10,
      max: 10000,
      label: "Height",
      description: "Height (in Pixels)"
    },
    widthFt: {
      type: "int",
      default: 6,
      min: 1,
      max: 500,
      label: "Width",
      description: "Width (in Feet)"
    },
    heightFt: {
      type: "int",
      default: 4,
      min: 1,
      max: 500,
      label: "Height",
      description: "Height (in Feet)"
    }
  },
  components: [
    {
      type: "strip",
      numPoints: "$width",
      instances: "$height",
      spacing: "$widthFt*12/($width-1)",
      y: "$instance*$heightFt*12/($height-1)"
    }
  ]
}